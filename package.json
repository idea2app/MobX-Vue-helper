{
  "name": "mobx-vue-helper",
  "version": "0.1.0",
  "description": "MobX Class & JSX helpers for Vue 3 components",
  "keywords": [
    "mobx",
    "vue",
    "vue3",
    "decorator",
    "observer",
    "reactive"
  ],
  "license": "LGPL-2.1",
  "author": "idea2app",
  "homepage": "https://github.com/idea2app/MobX-Vue-helper#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idea2app/MobX-Vue-helper.git"
  },
  "bugs": {
    "url": "https://github.com/idea2app/MobX-Vue-helper/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "document": "typedoc src/",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "mobx": ">=6.11",
    "mobx-vue-lite": ">=0.4",
    "vue": ">=3",
    "vue-facing-decorator": ">=4",
    "web-utility": ">=4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typedoc": "^0.27.0",
    "typescript": "^5.7.0"
  }
}
